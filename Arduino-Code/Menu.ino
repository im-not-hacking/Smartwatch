void choosebitmap(int bitmap, int x, int y, int color, boolean BIG = false){
  switch (bitmap){
    case 0:
      drawlocation(x,y,color,BIG);
      break;
    case 1:
      drawheart(x,y,color, BIG);
      break;
    case 2:
      drawsun(x,y,color,BIG);
      break;
    case 3:
      drawmusic(x,y,color,BIG);
      break;
    case 4:
      drawpong(x,y,color,BIG);
      break;
    case 5:
      drawtimer(x,y,color,BIG);
      break;
    case 6: 
      drawlight(x,y,color,BIG);
      break;
    case 7:
      drawengine(x,y,color,BIG);
      break;
  }
}


