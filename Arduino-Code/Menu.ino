void hoverApp(int current, int prev = 0){

  Oled.fillCircle(posx[prev],posy[prev],17,BLACK);
  Oled.fillCircle(posx[prev],posy[prev],12,colorapp[prev]);
  choosebitmap(prev,posx[prev],posy[prev],colorapp[prev]);

  Oled.fillCircle(posx[current],posy[current],17,color);
  Oled.fillCircle(posx[current],posy[current],15,BLACK); 

  Oled.fillCircle(posx[current],posy[current],12,colorapp[current]); 
  choosebitmap(current,posx[current],posy[current],colorapp[current]);
}

void selectApp(int current){
  Oled.fillCircle(posx[current],posy[current],17,BLACK);
