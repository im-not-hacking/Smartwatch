void menu_(){

  selected = 0;
  
  int index = 0;
  int tri_inc=0;
  int y_inc = 0;

  
  beg_menuu:
    Oled.fillScreen(BLACK);
    Oled.setTextColor(color);
    Oled.setFont(&Orbitron_Bold_14);
    Oled.drawBitmap(40, 40, placitlogo, 50, 50, color);
    
  menuu:
    hoverApp(index);
    drawApps();
    
    wait_middle();
  
  waiting:
    wait_for_any_button();
  
  
    //// BUTTOM BUTTON
    if (digitalRead(b_button) ==  LOW){
      wait_bottom();
      if (index<7){
        index+=1;
        hoverApp(index, index-1);
      }
      else{
        index=0;
        hoverApp(index,7);
      }
      goto waiting;
    }
    //// END OF BOTTOM BUTTON
  
  
    //// TOP BUTTON
    else if (digitalRead(t_button) ==  LOW){
      wait_top();
      if (index>0){
        index-=1;
        hoverApp(index, index+1);
      }
      else{
        index=7;
        hoverApp(index,0);
      }
      goto waiting;
    }
    //// END OF TOP BUTTON
  
  
    //// MIDDLE BUTTON
    else if (digitalRead(m_button) ==  LOW){
      selectApp(index);
      wait = 0;
      while (digitalRead(m_button) ==  LOW){
        if (wait >= 1000){
          goto end_menu;
        }
        delay(10);
        wait += 10;
      }
      if(makeSound){buzzer();}
      delay(150);
      hoverApp(index);
      delay(250);
      selected = index;
      select_function(index);
      if (goto_main == 1){
        goto_main = 0;
        goto end_menu;
      }
      delay(250);
      goto beg_menuu;
    }
    //// END OF MIDDLE BUTTON


  end_menu:
    if(makeSound){buzzer();}
    Oled.fillScreen(BLACK);
    prevMinute = -1;
    prevHour = -1;
    prevSecond = -1;
    prevDay = "-1";
    v_prevhour = -1;
    v_prevminute = -1;
    v_prevsecond = -1;
    ampm = "None";
    selected = 0;
}
